{
    "$schema": "http://json.schemastore.org/template",
    "author": "Cyro",
    "classifications": [ "Library", "ResoniteMod" ],
    "identity": "Cyro.ResoniteModTemplate",
    "name": "Resonite Mod Template",
    "shortName": "rmltemplate",

    "sources": [
        {
            "exclude": [
                "**/[Bb]in/**",
                "**/[Oo]bj/**",
                ".template.config/**/*",
                "**/*.filelist",
                "**/*.user",
                "**/*.lock.json",
                "**/.git",
                "**/.git/**"
            ]
        }
    ],

    "symbols": {
        "templateNamespace": {
            "type": "parameter",
            "datatype": "text",
            "defaultValue": "MyNamespace",
            "replaces": "TemplateNamespace"
        },
        "className": {
            "type": "parameter",
            "fileRename": "ResoniteModTemplate",
            "datatype": "text",
            "defaultValue": "RMLMod",
            "replaces": "ResoniteModTemplate"
        },
        "modName": {
            "type": "parameter",
            "datatype": "text",
            "defaultValue": "My RML Mod",
            "replaces": "Resonite Mod Template"
        },
        "author": {
            "type": "parameter",
            "datatype": "text",
            "defaultValue": "You! :)",
            "replaces": "Owner"
        },
        "link": {
            "type": "parameter",
            "datatype": "text",
            "defaultValue": "resonite.com",
            "replaces": "resonite.com"
        }
    }
}
  